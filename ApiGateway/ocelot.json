{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/clientes/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cliente-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/clientes/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/api/cuentas/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cuenta-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/cuentas/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/swagger/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cliente-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/gateway/cliente/swagger/{everything}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/swagger/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cuenta-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/gateway/cuenta/swagger/{everything}",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "BaseUrl": "http://localhost:8080"
  }
}
